   "source": [
    "from PIL import Image\n",
    "\n",
    "im1 = Image.open('lena-gray_4.jpg')\n",
    "im1.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "im2 = Image.new('L',(im1.size[0]*8,im1.size[1]*8))\n",
    "for i in range(im2.size[0]):\n",
    "    for j in range(im2.size[1]):\n",
    "        im2.putpixel((i,j),im1.getpixel((i//8,j//8)))\n",
    "im2.show()"
   ]
  },
